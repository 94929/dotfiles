[core]
editor = vim
autocrlf = input

[alias]
a = add
b = branch
c = commit
d = diff
s = status
m = merge

ps = push
pl = pull
pso = push origin
plo = pull origin
psso = push --set-upstream origin
graph = log --all -15 --graph --decorate --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%Creset' --abbrev-commit --date=relative

r = remote
rb = rebase
f = fetch
co = checkout
sw = switch

[color]
ui = true

